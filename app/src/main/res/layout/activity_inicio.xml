        android:textSize="34sp"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="241dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="MainAct"
